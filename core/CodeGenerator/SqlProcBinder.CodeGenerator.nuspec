<?xml version="1.0" encoding="utf-8"?>
<package xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <metadata xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <id>@project@</id>
    <title>@project@@title@</title>
    <version>@build.number@</version>
    <authors>Esun Kim</authors>
    <owners>Esun Kim</owners>
    <description>Simple StoredProcedure Binder in a type safe way. This generates class codes to call stored procedures.</description>
    <licenseUrl>https://raw.githubusercontent.com/SaladLab/SqlProcBinder/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/SaladLab/SqlProcBinder</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/SaladLab/SqlProcBinder/master/docs/Icon_64.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <releaseNotes>@releaseNotes@</releaseNotes>
    <copyright>Copyright Â© 2016 SaladLab</copyright>
    <tags>sql stored procedure binder type safe</tags>
    @dependencies@
    @references@
  </metadata>
  <files>
    <!-- base path: /bin/nuget/work/id -->
    <file src="..\..\..\..\core\CodeGenerator\bin\Release\*.exe" target="tools" />
    <file src="..\..\..\..\core\CodeGenerator\bin\Release\*.dll" target="tools" />
    <file src="..\..\..\..\core\CodeGenerator\bin\Release\*.config" target="tools" />
  </files>
</package>
